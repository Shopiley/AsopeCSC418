    "import cv2\n",
    "\n",
    "path = 'img/vr_headset.jpeg'\n",
    "\n",
    "img = cv2.imread(path, 0)\n",
    "\n",
    "window_name = 'VR Headset'\n",
    "\n",
    "cv2.imshow(window_name, img)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "\n",
    "cv2.destroyAllWindows()"
